identity:AddressFacet a owl:Class ;
    rdfs:label "AddressFacet" ;
    rdfs:comment "An Address Facet is a grouping of characteristics unique to an administrative identifier for a geolocation associated with a specific identity." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <https://w3id.org/lmodel/uco-location/Location> ;
            owl:onProperty identity:address ],
        identity:IdentityFacet .

identity:AffiliationFacet a owl:Class ;
    rdfs:label "AffiliationFacet" ;
    rdfs:comment "An affiliation is a grouping of characteristics unique to the established affiliations of an entity." ;
    rdfs:subClassOf identity:IdentityFacet .

identity:BirthInformationFacet a owl:Class ;
    rdfs:label "BirthInformationFacet" ;
    rdfs:comment "Birth information is a grouping of characteristics unique to information pertaining to the birth of an entity." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:dateTime ;
            owl:onProperty identity:birthdate ],
        identity:IdentityFacet .

identity:CountryOfResidenceFacet a owl:Class ;
    rdfs:label "CountryOfResidenceFacet" ;
    rdfs:comment "Country of residence is a grouping of characteristics unique to information related to the country, or countries, where an entity resides." ;
    rdfs:subClassOf identity:IdentityFacet .

identity:EventsFacet a owl:Class ;
    rdfs:label "EventsFacet" ;
    rdfs:comment "Events is a grouping of characteristics unique to information related to specific relevant things that happen in the lifetime of an entity." ;
    rdfs:subClassOf identity:IdentityFacet .

identity:IdentifierFacet a owl:Class ;
    rdfs:label "IdentifierFacet" ;
    rdfs:comment "Identifier is a grouping of characteristics unique to information that uniquely and specifically identities an entity." ;
    rdfs:subClassOf identity:IdentityFacet .

identity:LanguagesFacet a owl:Class ;
    rdfs:label "LanguagesFacet" ;
    rdfs:comment "Languages is a grouping of characteristics unique to specific syntactically and grammatically standardized forms of communication (human or computer) in which an entity has proficiency (comprehends, speaks, reads, or writes)." ;
    rdfs:subClassOf identity:IdentityFacet .

identity:NationalityFacet a owl:Class ;
    rdfs:label "NationalityFacet" ;
    rdfs:comment "Nationality is a grouping of characteristics unique to the condition of an entity belonging to a particular nation." ;
    rdfs:subClassOf identity:IdentityFacet .

identity:OccupationFacet a owl:Class ;
    rdfs:label "OccupationFacet" ;
    rdfs:comment "Occupation is a grouping of characteristics unique to the job or profession of an entity." ;
    rdfs:subClassOf identity:IdentityFacet .

identity:Organization a owl:Class ;
    rdfs:label "Organization" ;
    rdfs:comment "An organization is a grouping of identifying characteristics unique to a group of people who work together in an organized way for a shared purpose. [based on https://dictionary.cambridge.org/us/dictionary/english/organization]" ;
    rdfs:subClassOf identity:Identity ;
    skos:exactMatch SIO:000012,
        <http://identifiers.org/wikidata/Q15911314>,
        <http://identifiers.org/wikidata/Q43229>,
        GSSO:000232,
        sumo:Organization .

identity:OrganizationDetailsFacet a owl:Class ;
    rdfs:label "OrganizationDetailsFacet" ;
    rdfs:comment "Organization details is a grouping of characteristics unique to an identity representing an administrative and functional structure." ;
    rdfs:subClassOf identity:IdentityFacet .

identity:Person a owl:Class ;
    rdfs:label "Person" ;
    rdfs:comment "A person is a grouping of identifying characteristics unique to a human being regarded as an individual. [based on https://www.lexico.com/en/definition/person]" ;
    rdfs:subClassOf identity:Identity ;
    skos:exactMatch SIO:000498,
        <http://identifiers.org/wikidata/Q215627>,
        ns1:NCIT_C25190 ;
    skos:narrowMatch sumo:PersonID .

identity:PersonalDetailsFacet a owl:Class ;
    rdfs:label "PersonalDetailsFacet" ;
    rdfs:comment "Personal details is a grouping of characteristics unique to an identity representing an individual person." ;
    rdfs:subClassOf identity:IdentityFacet .

identity:PhysicalInfoFacet a owl:Class ;
    rdfs:label "PhysicalInfoFacet" ;
    rdfs:comment "Physical info is a grouping of characteristics unique to the outwardly observable nature of an individual person." ;
    rdfs:subClassOf identity:IdentityFacet .

identity:QualificationFacet a owl:Class ;
    rdfs:label "QualificationFacet" ;
    rdfs:comment "Qualification is a grouping of characteristics unique to particular skills, capabilities or their related achievements (educational, professional, etc.) of an entity." ;
    rdfs:subClassOf identity:IdentityFacet .

identity:RelatedIdentityFacet a owl:Class ;
    rdfs:label "RelatedIdentityFacet" ;
    rdfs:comment "<Needs fleshed out from CIQ>" ;
    rdfs:subClassOf identity:IdentityFacet .

identity:SimpleNameFacet a owl:Class ;
    rdfs:label "SimpleNameFacet" ;
    rdfs:comment "A simple name facet is a grouping of characteristics unique to the personal name (e.g., Dr. John Smith Jr.) held by an identity." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty identity:honorificPrefix ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty identity:givenName ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty identity:honorificSuffix ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty identity:familyName ],
        identity:IdentityFacet .

identity:VisaFacet a owl:Class ;
    rdfs:label "VisaFacet" ;
    rdfs:comment "Visa is a grouping of characteristics unique to information related to a person's ability to enter, leave, or stay for a specified period of time in a country." ;
    rdfs:subClassOf identity:IdentityFacet .

<https://w3id.org/lmodel/uco-location/GPSCoordinatesFacet> a owl:Class ;
    rdfs:label "GPSCoordinatesFacet" ;
    rdfs:comment "A GPS coordinates facet is a grouping of characteristics unique to the expression of quantified dilution of precision (DOP) for an asserted set of geolocation coordinates typically associated with satellite navigation such as the Global Positioning System (GPS)." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:decimal ;
            owl:onProperty <https://w3id.org/lmodel/uco-location/tdop> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:decimal ;
            owl:onProperty <https://w3id.org/lmodel/uco-location/hdop> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:decimal ;
            owl:onProperty <https://w3id.org/lmodel/uco-location/pdop> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:decimal ;
            owl:onProperty <https://w3id.org/lmodel/uco-location/vdop> ],
        <https://w3id.org/lmodel/uco-core/Facet> .

<https://w3id.org/lmodel/uco-location/LatLongCoordinatesFacet> a owl:Class ;
    rdfs:label "LatLongCoordinatesFacet" ;
    rdfs:comment "A lat long coordinates facet is a grouping of characteristics unique to the expression of a geolocation as the intersection of specific latitude, longitude, and altitude values." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:decimal ;
            owl:onProperty <https://w3id.org/lmodel/uco-location/latitude> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:decimal ;
            owl:onProperty <https://w3id.org/lmodel/uco-location/longitude> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:decimal ;
            owl:onProperty <https://w3id.org/lmodel/uco-location/altitude> ],
        <https://w3id.org/lmodel/uco-core/Facet> .

<https://w3id.org/lmodel/uco-location/SimpleAddressFacet> a owl:Class ;
    rdfs:label "SimpleAddressFacet" ;
    rdfs:comment "A simple address facet is a grouping of characteristics unique to a geolocation expressed as an administrative address." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:string ;
            owl:onProperty <https://w3id.org/lmodel/uco-location/region> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:string ;
            owl:onProperty <https://w3id.org/lmodel/uco-location/addressType> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:string ;
            owl:onProperty <https://w3id.org/lmodel/uco-location/postalCode> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:string ;
            owl:onProperty <https://w3id.org/lmodel/uco-location/street> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:string ;
            owl:onProperty <https://w3id.org/lmodel/uco-location/locality> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:string ;
            owl:onProperty <https://w3id.org/lmodel/uco-location/country> ],
        <https://w3id.org/lmodel/uco-core/Facet> .

<https://w3id.org/lmodel/uco-core/Assertion> a owl:Class ;
    rdfs:label "Assertion" ;
    rdfs:comment "An assertion is a statement declared to be true." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty <https://w3id.org/lmodel/uco-core/statement> ],
        <https://w3id.org/lmodel/uco-core/UcoObject> ;
    skos:closeMatch <http://identifiers.org/wikidata/Q843812> ;
    skos:exactMatch <http://identifiers.org/wikidata/Q814441>,
        ns1:SEPIO_0000001 .

<https://w3id.org/lmodel/uco-core/ContextualCompilation> a owl:Class ;
    rdfs:label "ContextualCompilation" ;
    rdfs:comment "A contextual compilation is a grouping of things sharing some context (e.g., a set of network connections observed on a given day, all accounts associated with a given person)." ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom <https://w3id.org/lmodel/uco-core/UcoObject> ;
                        owl:onProperty <https://w3id.org/lmodel/uco-core/object> ] [ a owl:Restriction ;
                        owl:onProperty <https://w3id.org/lmodel/uco-core/object> ;
                        owl:someValuesFrom <https://w3id.org/lmodel/uco-core/UcoObject> ] ) ],
        <https://w3id.org/lmodel/uco-core/Compilation> ;
    skos:relatedMatch <http://identifiers.org/wikidata/Q498552>,
        <http://identifiers.org/wikidata/Q5165203> .

<https://w3id.org/lmodel/uco-core/EnclosingCompilation> a owl:Class ;
    rdfs:label "EnclosingCompilation" ;
    rdfs:comment "An enclosing compilation is a container for a grouping of things." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/lmodel/uco-core/UcoObject> ;
            owl:onProperty <https://w3id.org/lmodel/uco-core/object> ],
        <https://w3id.org/lmodel/uco-core/Compilation> ;
    skos:narrowMatch <http://identifiers.org/wikidata/Q1643805>,
        <http://identifiers.org/wikidata/Q17008256> .

<https://w3id.org/lmodel/uco-core/ExternalReference> a owl:Class ;
    rdfs:label "ExternalReference" ;
    rdfs:comment "Characteristics of a reference to a resource outside of the UCO." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:string ;
            owl:onProperty <https://w3id.org/lmodel/uco-core/definingContext> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:anyURI ;
            owl:onProperty <https://w3id.org/lmodel/uco-core/referenceURL> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:string ;
            owl:onProperty <https://w3id.org/lmodel/uco-core/externalIdentifier> ],
        <https://w3id.org/lmodel/uco-core/UcoInherentCharacterizationThing> .

<https://w3id.org/lmodel/uco-core/confidence> a owl:DatatypeProperty ;
    rdfs:label "confidence" ;
    rdfs:comment "An asserted level of certainty in the accuracy of some information." ;
    rdfs:range xsd:nonNegativeInteger ;
    skos:closeMatch <http://identifiers.org/wikidata/Q4116378> ;
    skos:relatedMatch <http://identifiers.org/wikidata/Q1048607> .

<https://w3id.org/lmodel/uco-core/constrainingVocabularyName> a owl:DatatypeProperty ;
    rdfs:label "constrainingVocabularyName" ;
    rdfs:comment "The name of an explicitly constrained set of string values." ;
    rdfs:range xsd:string .

<https://w3id.org/lmodel/uco-core/constrainingVocabularyReference> a owl:DatatypeProperty ;
    rdfs:label "constrainingVocabularyReference" ;
    rdfs:comment "A reference to a specification for an explicitly constrained set of string values. The specification may be unstructured (e.g., web page listing string values) or structured (e.g. RDF/OWL enumeration)." ;
    rdfs:range xsd:anyURI .

<https://w3id.org/lmodel/uco-core/context> a owl:DatatypeProperty ;
    rdfs:label "context" ;
    rdfs:comment "A description of particular contextual affinity." ;
    rdfs:range xsd:string ;
    skos:exactMatch <http://identifiers.org/wikidata/Q813912>,
        ns1:NCIT_C63325 ;
    skos:narrowMatch <http://identifiers.org/wikidata/Q196626> ;
    skos:relatedMatch <http://identifiers.org/wikidata/Q2141565> .

<https://w3id.org/lmodel/uco-core/createdBy> a owl:DatatypeProperty ;
    rdfs:label "createdBy" ;
    rdfs:comment "The identity that created a characterization of a concept." ;
    rdfs:domain <https://w3id.org/lmodel/uco-core/IdentityAbstraction> ;
    rdfs:range xsd:string ;
    skos:exactMatch ns1:NCIT_C42628 ;
    skos:narrowMatch lm_core:author .

<https://w3id.org/lmodel/uco-core/definingContext> a owl:DatatypeProperty ;
    rdfs:label "definingContext" ;
    rdfs:comment "A description of the context relevant to the definition of a particular external reference identifier." ;
    rdfs:range xsd:string .

<https://w3id.org/lmodel/uco-core/endTime> a owl:DatatypeProperty ;
    rdfs:label "endTime" ;
    rdfs:comment "The ending time of a time range." ;
    rdfs:range xsd:dateTime ;
    skos:broadMatch sumo:TimePoint ;
    skos:exactMatch SIO:000670,
        <http://identifiers.org/wikidata/Q24575125>,
        ns1:NCIT_C82573,
        sumo:TimePoint .

<https://w3id.org/lmodel/uco-core/externalIdentifier> a owl:DatatypeProperty ;
    rdfs:label "externalIdentifier" ;
    rdfs:comment "An identifier for some information defined external to the UCO context." ;
    rdfs:range xsd:string .

<https://w3id.org/lmodel/uco-core/externalReference> a owl:DatatypeProperty ;
    rdfs:label "externalReference" ;
    rdfs:comment "Specifies a reference to a resource outside of the UCO." ;
    rdfs:domain <https://w3id.org/lmodel/uco-core/ExternalReference> ;
    rdfs:range xsd:string .

<https://w3id.org/lmodel/uco-core/hasFacet> a owl:DatatypeProperty ;
    rdfs:label "hasFacet" ;
    rdfs:comment "Further sets of properties characterizing a concept based on the particular context of the class and of the particular instance of the concept being characterized." ;
    rdfs:domain <https://w3id.org/lmodel/uco-core/Facet> ;
    rdfs:range xsd:string ;
    identity:owl "InverseFunctionalProperty" .

<https://w3id.org/lmodel/uco-core/isDirectional> a owl:DatatypeProperty ;
    rdfs:label "isDirectional" ;
